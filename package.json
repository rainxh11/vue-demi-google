{
  "name": "vue-demi-google",
  "version": "1.0.0",
  "description": "Google Login for Vue 2 & Vue 3",
  "type":"module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test_code": "npx ts-node ./test/test.ts",
    "test": "jest --config jestconfig.json",
    "build": "npx tsc -b",
    "dev": "vite --port 8080",
    "rollup": "rollup -c",
    "docs:dev": "vuepress dev documentation",
    "docs:build": "vuepress build documentation",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainxh11/vue-demi-google.git"
  },
  "publishConfig": {
    "@rainxh11:registry":"https://npm.pkg.github.com"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "vue",
    "google",
    "vue-demi",
    "onetap"
  ],
  "author": "Ahmed Chakhoum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rainxh11/vue-demi-google/issues"
  },
  "homepage": "https://github.com/rainxh11/vue-demi-google#readme",
  "dependencies": {
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "rollup": "^2.73.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.7",
    "vuepress": "^2.0.0-beta.43",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  }
}
